    "regenerator-runtime": "^0.13.9",
    "supertest": "^6.2.2",
    "testcontainers": "^8.2.0",
    "ts-node": "^10.7.0"
  },
  "mikro-orm": {
    "configPaths": [
      "./src/mikro-orm.config.ts",
      "./dist/mikro-orm.config.js"
    ]
  }
}
