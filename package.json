{
  "name": "main-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "remove-branches": "git branch | grep -v \"main\" | xargs git branch -D",
    "prebuild": "rimraf dist",
    "build": "swc src -d ./dist -D",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "swc src -s -d ./dist -D && node dist/main.js",
    "start:dev": "nodemon --watch src/ --exec \"npx swc src -s -d ./dist -D && node dist/main.js\" -e ts",
    "start:prod": "node dist/main.js",
    "migration:compile": "rimraf ./dist/migrations && swc ./migrations -D --include-dotfiles -d ./dist/migrations",
    "migration:create": "MIKRO_ORM_CLI_USE_TS_NODE=true mikro-orm migration:create && pnpm run migration:compile",
    "migration:up": "MIKRO_ORM_VERBOSE=verbose mikro-orm migration:up",
    "migration:down": "MIKRO_ORM_VERBOSE=verbose mikro-orm migration:down",
    "tsmigration:up": "MIKRO_ORM_CLI_USE_TS_NODE=true MIKRO_ORM_VERBOSE=verbose mikro-orm migration:up",
    "tsmigration:down": "MIKRO_ORM_CLI_USE_TS_NODE=true MIKRO_ORM_VERBOSE=verbose mikro-orm migration:down",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --config test/jest.unit.config.js",
    "test:integration": "jest --config test/jest.int.config.js --runInBand",
    "test:watch": "jest --projects test/jest.unit.config.js test/jest.int.config.js  test/jest.e2e.config.js --watch --runInBand",
    "test:cov": "jest --projects test/jest.unit.config.js test/jest.int.config.js test/jest.e2e.config.js --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r @swc/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config test/jest.e2e.config.js",
    "test:type": "tsc",
    "codegen": "openapi-generator-cli generate",
    "postcodegen": "pnpm format"
  },
  "dependencies": {
    "@casl/ability": "^5.4.3",
    "@mikro-orm/cli": "^5.0.3",
    "@mikro-orm/core": "^5.0.3",
    "@mikro-orm/knex": "^5.0.3",
    "@mikro-orm/migrations": "^5.0.3",
    "@mikro-orm/nestjs": "^5.0.0",
    "@mikro-orm/postgresql": "^5.0.3",
    "@nestjs/apollo": "^10.0.3",
    "@nestjs/axios": "^0.0.5",
    "@nestjs/common": "^8.3.1",
    "@nestjs/config": "^1.2.0",
    "@nestjs/core": "^8.3.1",
    "@nestjs/event-emitter": "^1.0.0",
    "@nestjs/graphql": "^10.0.3",
    "@nestjs/jwt": "^8.0.1",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.3.1",
    "@nestjs/platform-fastify": "^8.3.1",
    "@nestjs/schedule": "^1.0.2",
    "@nestjs/swagger": "^5.2.0",
    "@swc/cli": "^0.1.55",
    "@swc/core": "^1.2.154",
    "@swc/register": "^0.1.10",
    "amazon-cognito-identity-js": "^5.2.6",
    "apollo-server-core": "^3.6.3",
    "apollo-server-fastify": "^3.6.3",
    "axios": "^0.26.0",
