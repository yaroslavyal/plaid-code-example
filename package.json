    "graphql": "^15.8.0",
    "graphql-type-json": "^0.3.2",
    "i18n-iso-countries": "^7.4.0",
    "jwks-rsa": "^2.0.5",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.1",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "plaid": "^10.5.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
